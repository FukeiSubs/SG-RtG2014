Dialogue: 0,1:33:32.60,1:33:35.49,Subtitle,,0,0,0,,Tobira wo aketa ano shunkan kara
Dialogue: 0,1:33:35.49,1:33:38.36,Subtitle,,0,0,0,,subete ga ugokidashita ki ga shiteta
Dialogue: 0,1:33:38.36,1:33:41.16,Subtitle,,0,0,0,,Iki tsuku hima mo naku hashirinukete
Dialogue: 0,1:33:41.16,1:33:47.22,Subtitle,,0,0,0,,Kizande kita mujuuryoku juudai deizu
Dialogue: 0,1:33:55.24,1:33:58.10,Subtitle,,0,0,0,,"Marude shinderera" nante iu kedo
Dialogue: 0,1:33:58.10,1:34:00.93,Subtitle,,0,0,0,,hana wa ichinichi ni shite sakazu desho?
Dialogue: 0,1:34:00.93,1:34:03.74,Subtitle,,0,0,0,,Iccho mae ni tameiki wo tsuite mo
Dialogue: 0,1:34:03.74,1:34:06.57,Subtitle,,0,0,0,,madamada otona ni narenai
Dialogue: 0,1:34:06.57,1:34:09.40,Subtitle,,0,0,0,,Kyoudai no you na kyori de ireru kara
Dialogue: 0,1:34:09.40,1:34:12.24,Subtitle,,0,0,0,,yowane mo hakeba amae mo suru no yo
Dialogue: 0,1:34:12.24,1:34:15.08,Subtitle,,0,0,0,,Guutara na watashi shikaru toki dake
Dialogue: 0,1:34:15.08,1:34:17.85,Subtitle,,0,0,0,,kyuu ni maji ni naru
Dialogue: 0,1:34:18.21,1:34:23.90,Subtitle,,0,0,0,,Yama no toki mo tani no toki mo kimi to waratteitai
Dialogue: 0,1:34:23.90,1:34:28.78,Subtitle,,0,0,0,,Kotoba tarazu gozonji desho? Soredemo tsutaetai
Dialogue: 0,1:34:28.78,1:34:31.57,Subtitle,,0,0,0,,Kimi e no omoi
Dialogue: 0,1:34:31.57,1:34:35.32,Subtitle,,0,0,0,,omoi no take tsumekonda
Dialogue: 0,1:34:35.32,1:34:40.13,Subtitle,,0,0,0,,utagoe bakuon de hibikasete
Dialogue: 0,1:34:40.13,1:34:42.93,Subtitle,,0,0,0,,Kimi e todoke
Dialogue: 0,1:34:42.93,1:34:46.57,Subtitle,,0,0,0,,Ichiban boshi miageteru
Dialogue: 0,1:34:46.57,1:34:51.26,Subtitle,,0,0,0,,yuuzora orenji no kumo koete
Dialogue: 0,1:34:51.90,1:34:57.56,Subtitle,,0,0,0,,Dare yori mo massugu na kimi shitteru kara
Dialogue: 0,1:34:57.56,1:35:02.29,Subtitle,,0,0,0,,chanto kiite ite ne watashi shijou saikou no
Dialogue: 0,1:35:02.29,1:35:03.40,Subtitle,,0,0,0,,ARIGATO!
Dialogue: 0,1:35:14.29,1:35:17.73,Subtitle,,0,0,0,,Muteppou na kurai ga choudo ii nante
Dialogue: 0,1:35:17.73,1:35:19.97,Subtitle,,0,0,0,,Iroiro tameshite miru kedo
Dialogue: 0,1:35:19.97,1:35:22.81,Subtitle,,0,0,0,,tsumasakidachi de senobi wo shitatte
Dialogue: 0,1:35:22.81,1:35:25.62,Subtitle,,0,0,0,,sugu ni shibirete yorokechau
Dialogue: 0,1:35:25.62,1:35:28.48,Subtitle,,0,0,0,,Rouka no kagami miru tabi kanjiteru
Dialogue: 0,1:35:28.48,1:35:31.29,Subtitle,,0,0,0,,"Jibun rashisa wa doko ni aru n daro?"
Dialogue: 0,1:35:31.29,1:35:34.13,Subtitle,,0,0,0,,tenparu watashi wo yasashiku nadeta
Dialogue: 0,1:35:34.13,1:35:36.93,Subtitle,,0,0,0,,kimi ni toikakeru
Dialogue: 0,1:35:37.25,1:35:42.80,Subtitle,,0,0,0,,Hare no toki mo ame no toki mo koukai wa shitakunai
Dialogue: 0,1:35:42.80,1:35:47.92,Subtitle,,0,0,0,,Susumu yuuki kureteru no wa hoka no dare demo nai
Dialogue: 0,1:35:47.92,1:35:50.60,Subtitle,,0,0,0,,Kimi e todoke
Dialogue: 0,1:35:50.60,1:35:54.32,Subtitle,,0,0,0,,Sono hitomi wo kagayakase
Dialogue: 0,1:35:54.32,1:35:59.13,Subtitle,,0,0,0,,zenshin zenrei de mae muite
