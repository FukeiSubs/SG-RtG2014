Dialogue: 0,1:15:34.68,1:15:40.84,Subtitle,,0,0,0,,Ano hi no kagami ni utsuru mabushii shiruetto
Dialogue: 0,1:15:40.84,1:15:46.99,Subtitle,,0,0,0,,Karen na sugata akogareteita watashi ga ita
Dialogue: 0,1:15:46.99,1:15:53.14,Subtitle,,0,0,0,,“Mujaki” tte kotoba kara hamidasu kimi no koto wo
Dialogue: 0,1:15:53.14,1:15:59.14,Subtitle,,0,0,0,,tashinameru no mo keshikakeru no mo kuse ni natta
Dialogue: 0,1:15:59.14,1:16:05.46,Subtitle,,0,0,0,,Nani mo iwanakute mo naze ka tsutawarunda
Dialogue: 0,1:16:05.46,1:16:10.88,Subtitle,,0,0,0,,Kidoairaku mo kono kinchou mo kimi no shoi desho!?
Dialogue: 0,1:16:11.34,1:16:15.05,Subtitle,,0,0,0,,Ippai waraiatte shigeki shiatte
Dialogue: 0,1:16:15.05,1:16:18.28,Subtitle,,0,0,0,,kasaneta kanjou mirufiiyu
Dialogue: 0,1:16:18.28,1:16:23.60,Subtitle,,0,0,0,,Kanaderunda nijiiro no haamonii
Dialogue: 0,1:16:23.60,1:16:27.45,Subtitle,,0,0,0,,Baibai kuyashikutte namida nagashita
Dialogue: 0,1:16:27.45,1:16:35.46,Subtitle,,0,0,0,,Bukiyou na watashi ni ima wa nakanaide ieru yo
Dialogue: 0,1:16:35.46,1:16:37.18,Subtitle,,0,0,0,,"Jaa, mata ashita"
Dialogue: 0,1:16:48.49,1:16:54.72,Subtitle,,0,0,0,,Saikin no kimi wa sa sora dake wo mitsumete
Dialogue: 0,1:16:54.72,1:17:00.74,Subtitle,,0,0,0,,Yuruginai me de mugen no yume wo katatteru kedo
Dialogue: 0,1:17:00.74,1:17:06.92,Subtitle,,0,0,0,,ribon wo hodoitara otona ni naru aizu
Dialogue: 0,1:17:06.92,1:17:12.94,Subtitle,,0,0,0,,Oite ikanaide sonna yowane ga koboresou da yo
Dialogue: 0,1:17:12.94,1:17:19.26,Subtitle,,0,0,0,,Naraberu hiza to hiza tsutawaru nukumori ni
Dialogue: 0,1:17:19.26,1:17:24.64,Subtitle,,0,0,0,,Muki ni natte tsuyogaru kokoro yasashiku toketa
Dialogue: 0,1:17:25.20,1:17:28.98,Subtitle,,0,0,0,,Ippai fuzakeatte okoraretatte
Dialogue: 0,1:17:28.98,1:17:32.13,Subtitle,,0,0,0,,kuzurenai kanjou mirufiiyu
Dialogue: 0,1:17:32.13,1:17:37.44,Subtitle,,0,0,0,,Kono puraido dare ni mo kowasenai
Dialogue: 0,1:17:37.44,1:17:41.25,Subtitle,,0,0,0,,Zettai! jibun rashiku tsutaetainda
Dialogue: 0,1:17:41.25,1:17:49.26,Subtitle,,0,0,0,,Mikansei da to shite mo hazukashi garazu ni iu kara
Dialogue: 0,1:17:49.26,1:17:51.10,Subtitle,,0,0,0,,"Itsumo, arigatou"
Dialogue: 0,1:18:16.05,1:18:22.34,Subtitle,,0,0,0,,Hajimari no akashi ni soroeta sutorappu
Dialogue: 0,1:18:22.34,1:18:27.81,Subtitle,,0,0,0,,kimi ga kureta toki kara wa mou iroaseta kedo
Dialogue: 0,1:18:28.34,1:18:32.09,Subtitle,,0,0,0,,Isshou waraiatte shigeki shiatte
Dialogue: 0,1:18:32.09,1:18:35.17,Subtitle,,0,0,0,,kasanetai yuujou mirufiiyu
Dialogue: 0,1:18:35.17,1:18:40.60,Subtitle,,0,0,0,,Sekaijuu ni nijiiro no haamonii
Dialogue: 0,1:18:40.60,1:18:44.36,Subtitle,,0,0,0,,Baibai kuyashikutte namida nagashita
Dialogue: 0,1:18:44.36,1:18:52.32,Subtitle,,0,0,0,,Bukiyou na watashi ni ima wa nakanaide ieru yo
Dialogue: 0,1:18:52.32,1:18:54.00,Subtitle,,0,0,0,,"Jaa, mata ashita"
